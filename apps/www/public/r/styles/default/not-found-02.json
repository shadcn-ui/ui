{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "not-found-02",
  "type": "registry:block",
  "author": "shadcn (https://ui.shadcn.com)",
  "description": "A simple not found page split with an image.",
  "registryDependencies": [
    "button"
  ],
  "files": [
    {
      "path": "blocks/not-found-02/page.tsx",
      "content": "import { NotFound } from \"@/registry/default/blocks/not-found-02/components/not-found\"\n\nexport default function Page() {\n  return (\n    <div className=\"flex flex-col xl:grid min-h-svh xl:grid-cols-[max(50%,36rem),1fr] gap-y-16 xl:gap-0 p-6 pt-16 xl:p-0\">\n      <div className=\"flex flex-col items-center justify-center\">\n        <NotFound />\n      </div>\n      <div className=\"relative bg-muted h-96 xl:h-full mx-auto w-full max-w-3xl xl:max-w-full\">\n        <img\n          src=\"/placeholder.svg\"\n          alt=\"Image\"\n          className=\"absolute inset-0 h-full w-full object-cover dark:brightness-[0.2] dark:grayscale\"\n        />\n      </div>\n    </div>\n  )\n}\n",
      "type": "registry:page",
      "target": "app/not-found/page.tsx"
    },
    {
      "path": "blocks/not-found-02/components/not-found.tsx",
      "content": "import { Button } from \"@/registry/default/ui/button\"\n\nexport function NotFound() {\n  return (\n    <div className=\"text-center\">\n      <p className=\"text-base font-semibold text-primary\">404</p>\n      <h1 className=\"mt-4 text-balance text-5xl font-semibold tracking-tight text-primary sm:text-7xl\">\n        Page not found\n      </h1>\n      <p className=\"mt-6 text-pretty text-lg font-medium text-muted-foreground sm:text-xl/8\">\n        Lost, this page is. In another system, it may be.\n      </p>\n      <div className=\"mt-10 flex flex-col sm:flex-row sm:items-center sm:justify-center gap-y-3 gap-x-6\">\n        <Button variant=\"secondary\" asChild>\n          <a href=\"#\">\n            <span aria-hidden=\"true\">&larr;</span> Go back\n          </a>\n        </Button>\n        <Button className=\"-order-1 sm:order-none\" asChild>\n          <a href=\"#\">Take me home</a>\n        </Button>\n      </div>\n    </div>\n  )\n}\n",
      "type": "registry:component",
      "target": ""
    }
  ],
  "categories": [
    "404-pages"
  ]
}
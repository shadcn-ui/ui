  "files": [
    {
      "path": "registry/new-york-v4/examples/empty-icon.tsx",
      "content": "import {\r\n  IconBookmark,\r\n  IconHeart,\r\n  IconInbox,\r\n  IconStar,\r\n} from \"@tabler/icons-react\"\r\n\r\nimport {\r\n  Empty,\r\n  EmptyDescription,\r\n  EmptyHeader,\r\n  EmptyMedia,\r\n  EmptyTitle,\r\n} from \"@/registry/new-york-v4/ui/empty\"\r\n\r\nexport default function EmptyIcon() {\r\n  return (\r\n    <div className=\"grid gap-8 md:grid-cols-2\">\r\n      <Empty>\r\n        <EmptyHeader>\r\n          <EmptyMedia variant=\"icon\">\r\n            <IconInbox />\r\n          </EmptyMedia>\r\n          <EmptyTitle>No messages</EmptyTitle>\r\n          <EmptyDescription>\r\n            Your inbox is empty. New messages will appear here.\r\n          </EmptyDescription>\r\n        </EmptyHeader>\r\n      </Empty>\r\n\r\n      <Empty>\r\n        <EmptyHeader>\r\n          <EmptyMedia variant=\"icon\">\r\n            <IconStar />\r\n          </EmptyMedia>\r\n          <EmptyTitle>No favorites</EmptyTitle>\r\n          <EmptyDescription>\r\n            Items you mark as favorites will appear here.\r\n          </EmptyDescription>\r\n        </EmptyHeader>\r\n      </Empty>\r\n\r\n      <Empty>\r\n        <EmptyHeader>\r\n          <EmptyMedia variant=\"icon\">\r\n            <IconHeart />\r\n          </EmptyMedia>\r\n          <EmptyTitle>No likes yet</EmptyTitle>\r\n          <EmptyDescription>\r\n            Content you like will be saved here for easy access.\r\n          </EmptyDescription>\r\n        </EmptyHeader>\r\n      </Empty>\r\n\r\n      <Empty>\r\n        <EmptyHeader>\r\n          <EmptyMedia variant=\"icon\">\r\n            <IconBookmark />\r\n          </EmptyMedia>\r\n          <EmptyTitle>No bookmarks</EmptyTitle>\r\n          <EmptyDescription>\r\n            Save interesting content by bookmarking it.\r\n          </EmptyDescription>\r\n        </EmptyHeader>\r\n      </Empty>\r\n    </div>\r\n  )\r\n}\r\n",
      "type": "registry:example"
    }
  ],
  "type": "registry:example"
}
    {
      "path": "registry/radix-lyra/examples/kbd-example.tsx",
      "content": "import { IconArrowLeft, IconArrowRight } from \"@tabler/icons-react\"\n\nimport { Button } from \"@/registry/bases/radix/ui/button\"\nimport { ButtonGroup } from \"@/registry/bases/radix/ui/button-group\"\nimport {\n  InputGroup,\n  InputGroupAddon,\n  InputGroupInput,\n} from \"@/registry/bases/radix/ui/input-group\"\nimport { Kbd, KbdGroup } from \"@/registry/bases/radix/ui/kbd\"\nimport {\n  Tooltip,\n  TooltipContent,\n  TooltipTrigger,\n} from \"@/registry/bases/radix/ui/tooltip\"\nimport Frame from \"@/app/(design)/design/components/frame\"\nimport { IconPlaceholder } from \"@/app/(design)/design/components/icon-placeholder\"\n\nexport default function KbdExample() {\n  return (\n    <div className=\"bg-background flex min-h-screen items-center justify-center p-6 lg:p-12\">\n      <div className=\"w-full\">\n        <div className=\"grid grid-cols-1 items-start gap-6 md:grid-cols-2 md:gap-12 lg:grid-cols-3\">\n          <KbdBasic />\n          <KbdModifierKeys />\n          <KbdGroupExample />\n          <KbdArrowKeys />\n          <KbdWithIcons />\n          <KbdWithIconsAndText />\n          <KbdInInputGroup />\n          <KbdInTooltip />\n          <KbdWithSamp />\n        </div>\n      </div>\n    </div>\n  )\n}\n\nfunction KbdBasic() {\n  return (\n    <Frame title=\"Basic\">\n      <div className=\"flex items-center gap-2\">\n        <Kbd>Ctrl</Kbd>\n        <Kbd>⌘K</Kbd>\n        <Kbd>Ctrl + B</Kbd>\n      </div>\n    </Frame>\n  )\n}\n\nfunction KbdModifierKeys() {\n  return (\n    <Frame title=\"Modifier Keys\">\n      <div className=\"flex items-center gap-2\">\n        <Kbd>⌘</Kbd>\n        <Kbd>C</Kbd>\n      </div>\n    </Frame>\n  )\n}\n\nfunction KbdGroupExample() {\n  return (\n    <Frame title=\"KbdGroup\">\n      <KbdGroup>\n        <Kbd>Ctrl</Kbd>\n        <Kbd>Shift</Kbd>\n        <Kbd>P</Kbd>\n      </KbdGroup>\n    </Frame>\n  )\n}\n\nfunction KbdArrowKeys() {\n  return (\n    <Frame title=\"Arrow Keys\">\n      <div className=\"flex items-center gap-2\">\n        <Kbd>↑</Kbd>\n        <Kbd>↓</Kbd>\n        <Kbd>←</Kbd>\n        <Kbd>→</Kbd>\n      </div>\n    </Frame>\n  )\n}\n\nfunction KbdWithIcons() {\n  return (\n    <Frame title=\"With Icons\">\n      <KbdGroup>\n        <Kbd>\n          <IconPlaceholder\n            lucide=\"CircleDashedIcon\"\n            tabler=\"IconCircleDashed\"\n            hugeicons=\"DashedLineCircleIcon\"\n          />\n        </Kbd>\n        <Kbd>\n          <IconArrowLeft />\n        </Kbd>\n        <Kbd>\n          <IconArrowRight />\n        </Kbd>\n      </KbdGroup>\n    </Frame>\n  )\n}\n\nfunction KbdWithIconsAndText() {\n  return (\n    <Frame title=\"With Icons and Text\">\n      <KbdGroup>\n        <Kbd>\n          <IconArrowLeft />\n          Left\n        </Kbd>\n        <Kbd>\n          <IconPlaceholder\n            lucide=\"CircleDashedIcon\"\n            tabler=\"IconCircleDashed\"\n            hugeicons=\"DashedLineCircleIcon\"\n          />\n          Voice Enabled\n        </Kbd>\n      </KbdGroup>\n    </Frame>\n  )\n}\n\nfunction KbdInInputGroup() {\n  return (\n    <Frame title=\"InputGroup\">\n      <InputGroup>\n        <InputGroupInput />\n        <InputGroupAddon>\n          <Kbd>Space</Kbd>\n        </InputGroupAddon>\n      </InputGroup>\n    </Frame>\n  )\n}\n\nfunction KbdInTooltip() {\n  return (\n    <Frame title=\"Tooltip\">\n      <Tooltip>\n        <TooltipTrigger asChild>\n          <Button size=\"icon-sm\" variant=\"outline\">\n            <IconPlaceholder\n              lucide=\"SaveIcon\"\n              tabler=\"IconDeviceFloppy\"\n              hugeicons=\"FloppyDiskIcon\"\n            />\n          </Button>\n        </TooltipTrigger>\n        <TooltipContent>\n          <div className=\"flex items-center gap-2\">\n            Save Changes <Kbd>S</Kbd>\n          </div>\n        </TooltipContent>\n      </Tooltip>\n    </Frame>\n  )\n}\n\nfunction KbdWithSamp() {\n  return (\n    <Frame title=\"With samp\">\n      <Kbd>\n        <samp>File</samp>\n      </Kbd>\n    </Frame>\n  )\n}\n",
      "type": "registry:example"
    }
  ]
}
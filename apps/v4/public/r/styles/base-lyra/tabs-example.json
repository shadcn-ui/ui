{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "tabs-example",
  "title": "Tabs",
  "registryDependencies": [
    "button",
    "dropdown-menu",
    "tabs",
    "example"
  ],
  "files": [
    {
      "path": "registry/base-lyra/examples/tabs-example.tsx",
      "content": "import {\n  Example,\n  ExampleWrapper,\n} from \"@/registry/bases/base/components/example\"\nimport { Button } from \"@/registry/bases/base/ui/button\"\nimport {\n  DropdownMenu,\n  DropdownMenuContent,\n  DropdownMenuItem,\n  DropdownMenuSeparator,\n  DropdownMenuTrigger,\n} from \"@/registry/bases/base/ui/dropdown-menu\"\nimport { Input } from \"@/registry/bases/base/ui/input\"\nimport {\n  Tabs,\n  TabsContent,\n  TabsList,\n  TabsTrigger,\n} from \"@/registry/bases/base/ui/tabs\"\nimport { IconPlaceholder } from \"@/app/(create)/components/icon-placeholder\"\n\nexport default function TabsExample() {\n  return (\n    <ExampleWrapper>\n      <TabsBasic />\n      <TabsLine />\n      <TabsVariantsComparison />\n      <TabsDisabled />\n      <TabsWithIcons />\n      <TabsIconOnly />\n      <TabsMultiple />\n      <TabsWithContent />\n      <TabsLineWithContent />\n      <TabsLineDisabled />\n      <TabsWithDropdown />\n      <TabsVertical />\n      <TabsNestedLineInDefault />\n      <TabsNestedDefaultInLine />\n      <TabsWithInputAndButton />\n    </ExampleWrapper>\n  )\n}\n\nfunction TabsBasic() {\n  return (\n    <Example title=\"Basic\">\n      <Tabs defaultValue=\"home\">\n        <TabsList>\n          <TabsTrigger value=\"home\">Home</TabsTrigger>\n          <TabsTrigger value=\"settings\">Settings</TabsTrigger>\n        </TabsList>\n      </Tabs>\n    </Example>\n  )\n}\n\nfunction TabsLine() {\n  return (\n    <Example title=\"Line\">\n      <Tabs defaultValue=\"overview\">\n        <TabsList variant=\"line\">\n          <TabsTrigger value=\"overview\">Overview</TabsTrigger>\n          <TabsTrigger value=\"analytics\">Analytics</TabsTrigger>\n          <TabsTrigger value=\"reports\">Reports</TabsTrigger>\n        </TabsList>\n      </Tabs>\n    </Example>\n  )\n}\n\nfunction TabsVariantsComparison() {\n  return (\n    <Example title=\"Variants Alignment\">\n      <div className=\"flex gap-4\">\n        <Tabs defaultValue=\"overview\">\n          <TabsList>\n            <TabsTrigger value=\"overview\">Overview</TabsTrigger>\n            <TabsTrigger value=\"analytics\">Analytics</TabsTrigger>\n          </TabsList>\n        </Tabs>\n        <Tabs defaultValue=\"overview\">\n          <TabsList variant=\"line\">\n            <TabsTrigger value=\"overview\">Overview</TabsTrigger>\n            <TabsTrigger value=\"analytics\">Analytics</TabsTrigger>\n          </TabsList>\n        </Tabs>\n      </div>\n    </Example>\n  )\n}\n\nfunction TabsDisabled() {\n  return (\n    <Example title=\"Disabled\">\n      <Tabs defaultValue=\"home\">\n        <TabsList>\n          <TabsTrigger value=\"home\">Home</TabsTrigger>\n          <TabsTrigger value=\"settings\" disabled>\n            Disabled\n          </TabsTrigger>\n        </TabsList>\n      </Tabs>\n    </Example>\n  )\n}\n\nfunction TabsWithIcons() {\n  return (\n    <Example title=\"With Icons\">\n      <Tabs defaultValue=\"preview\">\n        <TabsList>\n          <TabsTrigger value=\"preview\">\n            <IconPlaceholder\n              lucide=\"AppWindowIcon\"\n              tabler=\"IconAppWindow\"\n              hugeicons=\"CursorInWindowIcon\"\n            />\n            Preview\n          </TabsTrigger>\n          <TabsTrigger value=\"code\">\n            <IconPlaceholder\n              lucide=\"CodeIcon\"\n              tabler=\"IconCode\"\n              hugeicons=\"CodeIcon\"\n            />\n            Code\n          </TabsTrigger>\n        </TabsList>\n      </Tabs>\n    </Example>\n  )\n}\n\nfunction TabsIconOnly() {\n  return (\n    <Example title=\"Icon Only\">\n      <Tabs defaultValue=\"home\">\n        <TabsList>\n          <TabsTrigger value=\"home\">\n            <IconPlaceholder\n              lucide=\"HomeIcon\"\n              tabler=\"IconHome\"\n              hugeicons=\"HomeIcon\"\n            />\n          </TabsTrigger>\n          <TabsTrigger value=\"search\">\n            <IconPlaceholder\n              lucide=\"SearchIcon\"\n              tabler=\"IconSearch\"\n              hugeicons=\"SearchIcon\"\n            />\n          </TabsTrigger>\n          <TabsTrigger value=\"settings\">\n            <IconPlaceholder\n              lucide=\"SettingsIcon\"\n              tabler=\"IconSettings\"\n              hugeicons=\"SettingsIcon\"\n            />\n          </TabsTrigger>\n        </TabsList>\n      </Tabs>\n    </Example>\n  )\n}\n\nfunction TabsMultiple() {\n  return (\n    <Example title=\"Multiple\">\n      <Tabs defaultValue=\"overview\">\n        <TabsList>\n          <TabsTrigger value=\"overview\">Overview</TabsTrigger>\n          <TabsTrigger value=\"analytics\">Analytics</TabsTrigger>\n          <TabsTrigger value=\"reports\">Reports</TabsTrigger>\n          <TabsTrigger value=\"settings\">Settings</TabsTrigger>\n        </TabsList>\n      </Tabs>\n    </Example>\n  )\n}\n\nfunction TabsWithContent() {\n  return (\n    <Example title=\"With Content\">\n      <Tabs defaultValue=\"account\">\n        <TabsList>\n          <TabsTrigger value=\"account\">Account</TabsTrigger>\n          <TabsTrigger value=\"password\">Password</TabsTrigger>\n          <TabsTrigger value=\"notifications\">Notifications</TabsTrigger>\n        </TabsList>\n        <TabsContent value=\"account\">\n          <div className=\"rounded-lg border p-4\">\n            <h3 className=\"mb-2 text-sm font-medium\">Account Settings</h3>\n            <p className=\"text-muted-foreground text-sm\">\n              Manage your account preferences and profile information.\n            </p>\n          </div>\n        </TabsContent>\n        <TabsContent value=\"password\">\n          <div className=\"rounded-lg border p-4\">\n            <h3 className=\"mb-2 text-sm font-medium\">Password Settings</h3>\n            <p className=\"text-muted-foreground text-sm\">\n              Update your password to keep your account secure.\n            </p>\n          </div>\n        </TabsContent>\n        <TabsContent value=\"notifications\">\n          <div className=\"rounded-lg border p-4\">\n            <h3 className=\"mb-2 text-sm font-medium\">Notification Settings</h3>\n            <p className=\"text-muted-foreground text-sm\">\n              Configure how you receive notifications and alerts.\n            </p>\n          </div>\n        </TabsContent>\n      </Tabs>\n    </Example>\n  )\n}\n\nfunction TabsLineWithContent() {\n  return (\n    <Example title=\"Line With Content\">\n      <Tabs defaultValue=\"account\">\n        <TabsList variant=\"line\">\n          <TabsTrigger value=\"account\">Account</TabsTrigger>\n          <TabsTrigger value=\"password\">Password</TabsTrigger>\n          <TabsTrigger value=\"notifications\">Notifications</TabsTrigger>\n        </TabsList>\n        <TabsContent value=\"account\">\n          <div className=\"rounded-lg border p-4\">\n            <h3 className=\"mb-2 text-sm font-medium\">Account Settings</h3>\n            <p className=\"text-muted-foreground text-sm\">\n              Manage your account preferences and profile information.\n            </p>\n          </div>\n        </TabsContent>\n        <TabsContent value=\"password\">\n          <div className=\"rounded-lg border p-4\">\n            <h3 className=\"mb-2 text-sm font-medium\">Password Settings</h3>\n            <p className=\"text-muted-foreground text-sm\">\n              Update your password to keep your account secure.\n            </p>\n          </div>\n        </TabsContent>\n        <TabsContent value=\"notifications\">\n          <div className=\"rounded-lg border p-4\">\n            <h3 className=\"mb-2 text-sm font-medium\">Notification Settings</h3>\n            <p className=\"text-muted-foreground text-sm\">\n              Configure how you receive notifications and alerts.\n            </p>\n          </div>\n        </TabsContent>\n      </Tabs>\n    </Example>\n  )\n}\n\nfunction TabsLineDisabled() {\n  return (\n    <Example title=\"Line Disabled\">\n      <Tabs defaultValue=\"overview\">\n        <TabsList variant=\"line\">\n          <TabsTrigger value=\"overview\">Overview</TabsTrigger>\n          <TabsTrigger value=\"analytics\">Analytics</TabsTrigger>\n          <TabsTrigger value=\"reports\" disabled>\n            Reports\n          </TabsTrigger>\n        </TabsList>\n      </Tabs>\n    </Example>\n  )\n}\n\nfunction TabsWithDropdown() {\n  return (\n    <Example title=\"With Dropdown\">\n      <Tabs defaultValue=\"overview\">\n        <div className=\"flex items-center justify-between\">\n          <TabsList>\n            <TabsTrigger value=\"overview\">Overview</TabsTrigger>\n            <TabsTrigger value=\"analytics\">Analytics</TabsTrigger>\n            <TabsTrigger value=\"reports\">Reports</TabsTrigger>\n          </TabsList>\n          <DropdownMenu>\n            <DropdownMenuTrigger\n              render={<Button variant=\"ghost\" size=\"icon\" className=\"size-8\" />}\n            >\n              <IconPlaceholder\n                lucide=\"MoreHorizontalIcon\"\n                tabler=\"IconDots\"\n                hugeicons=\"MoreHorizontalIcon\"\n              />\n              <span className=\"sr-only\">More options</span>\n            </DropdownMenuTrigger>\n            <DropdownMenuContent align=\"end\">\n              <DropdownMenuItem>Settings</DropdownMenuItem>\n              <DropdownMenuItem>Export</DropdownMenuItem>\n              <DropdownMenuSeparator />\n              <DropdownMenuItem>Archive</DropdownMenuItem>\n            </DropdownMenuContent>\n          </DropdownMenu>\n        </div>\n        <TabsContent value=\"overview\">\n          <div className=\"rounded-lg border p-4\">\n            <h3 className=\"mb-2 text-sm font-medium\">Overview</h3>\n            <p className=\"text-muted-foreground text-sm\">\n              View your dashboard metrics and key performance indicators.\n            </p>\n          </div>\n        </TabsContent>\n        <TabsContent value=\"analytics\">\n          <div className=\"rounded-lg border p-4\">\n            <h3 className=\"mb-2 text-sm font-medium\">Analytics</h3>\n            <p className=\"text-muted-foreground text-sm\">\n              Detailed analytics and insights about your data.\n            </p>\n          </div>\n        </TabsContent>\n        <TabsContent value=\"reports\">\n          <div className=\"rounded-lg border p-4\">\n            <h3 className=\"mb-2 text-sm font-medium\">Reports</h3>\n            <p className=\"text-muted-foreground text-sm\">\n              Generate and view custom reports.\n            </p>\n          </div>\n        </TabsContent>\n      </Tabs>\n    </Example>\n  )\n}\n\nfunction TabsVertical() {\n  return (\n    <Example title=\"Vertical\">\n      <Tabs defaultValue=\"account\" orientation=\"vertical\">\n        <TabsList>\n          <TabsTrigger value=\"account\">Account</TabsTrigger>\n          <TabsTrigger value=\"password\">Password</TabsTrigger>\n          <TabsTrigger value=\"notifications\">Notifications</TabsTrigger>\n        </TabsList>\n        <TabsContent value=\"account\">\n          <div className=\"rounded-lg border p-6\">\n            <h3 className=\"mb-4 text-base font-semibold\">Account Settings</h3>\n            <p className=\"text-muted-foreground mb-6 text-sm\">\n              Manage your account preferences and profile information.\n            </p>\n            <div className=\"flex flex-col gap-4\">\n              <div>\n                <label className=\"mb-2 block text-sm font-medium\">\n                  Display Name\n                </label>\n                <p className=\"text-muted-foreground text-sm\">\n                  Your display name appears on your profile and in comments.\n                </p>\n              </div>\n            </div>\n          </div>\n        </TabsContent>\n        <TabsContent value=\"password\">\n          <div className=\"rounded-lg border p-6\">\n            <h3 className=\"mb-4 text-base font-semibold\">Password Settings</h3>\n            <p className=\"text-muted-foreground mb-6 text-sm\">\n              Update your password to keep your account secure. Use a strong\n              password with a mix of letters, numbers, and symbols.\n            </p>\n            <div className=\"flex flex-col gap-4\">\n              <div>\n                <h4 className=\"mb-2 text-sm font-medium\">\n                  Password Requirements\n                </h4>\n                <ul className=\"text-muted-foreground ml-4 list-disc space-y-1 text-sm\">\n                  <li>At least 8 characters long</li>\n                  <li>Contains uppercase and lowercase letters</li>\n                  <li>Includes at least one number</li>\n                  <li>Contains at least one special character</li>\n                </ul>\n              </div>\n            </div>\n          </div>\n        </TabsContent>\n        <TabsContent value=\"notifications\">\n          <div className=\"rounded-lg border p-6\">\n            <h3 className=\"mb-4 text-base font-semibold\">\n              Notification Settings\n            </h3>\n            <p className=\"text-muted-foreground mb-6 text-sm\">\n              Configure how you receive notifications and alerts. Choose which\n              types of notifications you want to receive and how you want to\n              receive them.\n            </p>\n            <div className=\"flex flex-col gap-6\">\n              <div>\n                <h4 className=\"mb-3 text-sm font-medium\">\n                  Email Notifications\n                </h4>\n                <div className=\"flex flex-col gap-4\">\n                  <div className=\"flex items-center justify-between\">\n                    <span className=\"text-sm\">New comments</span>\n                    <span className=\"text-muted-foreground text-xs\">\n                      Enabled\n                    </span>\n                  </div>\n                  <div className=\"flex items-center justify-between\">\n                    <span className=\"text-sm\">Weekly digest</span>\n                    <span className=\"text-muted-foreground text-xs\">\n                      Enabled\n                    </span>\n                  </div>\n                  <div className=\"flex items-center justify-between\">\n                    <span className=\"text-sm\">Security alerts</span>\n                    <span className=\"text-muted-foreground text-xs\">\n                      Enabled\n                    </span>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </TabsContent>\n      </Tabs>\n    </Example>\n  )\n}\n\nfunction TabsNestedLineInDefault() {\n  return (\n    <Example title=\"Nested Line in Default\">\n      <Tabs defaultValue=\"overview\">\n        <TabsList>\n          <TabsTrigger value=\"overview\">Overview</TabsTrigger>\n          <TabsTrigger value=\"analytics\">Analytics</TabsTrigger>\n          <TabsTrigger value=\"settings\">Settings</TabsTrigger>\n        </TabsList>\n        <TabsContent value=\"overview\">\n          <div className=\"rounded-lg border p-4\">\n            <Tabs defaultValue=\"dashboard\">\n              <TabsList variant=\"line\">\n                <TabsTrigger value=\"dashboard\">Dashboard</TabsTrigger>\n                <TabsTrigger value=\"reports\">Reports</TabsTrigger>\n              </TabsList>\n              <TabsContent value=\"dashboard\">\n                <p className=\"text-muted-foreground text-sm\">\n                  Dashboard content goes here.\n                </p>\n              </TabsContent>\n              <TabsContent value=\"reports\">\n                <p className=\"text-muted-foreground text-sm\">\n                  Reports content goes here.\n                </p>\n              </TabsContent>\n            </Tabs>\n          </div>\n        </TabsContent>\n        <TabsContent value=\"analytics\">\n          <div className=\"rounded-lg border p-4\">\n            <h3 className=\"mb-2 text-sm font-medium\">Analytics</h3>\n            <p className=\"text-muted-foreground text-sm\">\n              View your analytics data here.\n            </p>\n          </div>\n        </TabsContent>\n        <TabsContent value=\"settings\">\n          <div className=\"rounded-lg border p-4\">\n            <h3 className=\"mb-2 text-sm font-medium\">Settings</h3>\n            <p className=\"text-muted-foreground text-sm\">\n              Configure your settings here.\n            </p>\n          </div>\n        </TabsContent>\n      </Tabs>\n    </Example>\n  )\n}\n\nfunction TabsNestedDefaultInLine() {\n  return (\n    <Example title=\"Nested Default in Line\">\n      <Tabs defaultValue=\"overview\">\n        <TabsList variant=\"line\">\n          <TabsTrigger value=\"overview\">Overview</TabsTrigger>\n          <TabsTrigger value=\"analytics\">Analytics</TabsTrigger>\n          <TabsTrigger value=\"settings\">Settings</TabsTrigger>\n        </TabsList>\n        <TabsContent value=\"overview\">\n          <div className=\"rounded-lg border p-4\">\n            <Tabs defaultValue=\"dashboard\">\n              <TabsList>\n                <TabsTrigger value=\"dashboard\">Dashboard</TabsTrigger>\n                <TabsTrigger value=\"reports\">Reports</TabsTrigger>\n              </TabsList>\n              <TabsContent value=\"dashboard\">\n                <p className=\"text-muted-foreground text-sm\">\n                  Dashboard content goes here.\n                </p>\n              </TabsContent>\n              <TabsContent value=\"reports\">\n                <p className=\"text-muted-foreground text-sm\">\n                  Reports content goes here.\n                </p>\n              </TabsContent>\n            </Tabs>\n          </div>\n        </TabsContent>\n        <TabsContent value=\"analytics\">\n          <div className=\"rounded-lg border p-4\">\n            <h3 className=\"mb-2 text-sm font-medium\">Analytics</h3>\n            <p className=\"text-muted-foreground text-sm\">\n              View your analytics data here.\n            </p>\n          </div>\n        </TabsContent>\n        <TabsContent value=\"settings\">\n          <div className=\"rounded-lg border p-4\">\n            <h3 className=\"mb-2 text-sm font-medium\">Settings</h3>\n            <p className=\"text-muted-foreground text-sm\">\n              Configure your settings here.\n            </p>\n          </div>\n        </TabsContent>\n      </Tabs>\n    </Example>\n  )\n}\n\nfunction TabsWithInputAndButton() {\n  return (\n    <Example title=\"With Input and Button\" containerClassName=\"col-span-full\">\n      <Tabs defaultValue=\"overview\" className=\"mx-auto w-full max-w-lg\">\n        <div className=\"flex items-center gap-4\">\n          <TabsList>\n            <TabsTrigger value=\"overview\">Overview</TabsTrigger>\n            <TabsTrigger value=\"analytics\">Analytics</TabsTrigger>\n          </TabsList>\n          <div className=\"ml-auto flex items-center gap-2\">\n            <Input placeholder=\"Search...\" className=\"w-44\" />\n            <Button>Action</Button>\n          </div>\n        </div>\n        <TabsContent value=\"overview\">\n          <div className=\"rounded-lg border p-4\">\n            <h3 className=\"mb-2 text-sm font-medium\">Overview</h3>\n            <p className=\"text-muted-foreground text-sm\">\n              View your dashboard metrics and key performance indicators.\n            </p>\n          </div>\n        </TabsContent>\n        <TabsContent value=\"analytics\">\n          <div className=\"rounded-lg border p-4\">\n            <h3 className=\"mb-2 text-sm font-medium\">Analytics</h3>\n            <p className=\"text-muted-foreground text-sm\">\n              Detailed analytics and insights about your data.\n            </p>\n          </div>\n        </TabsContent>\n        <TabsContent value=\"reports\">\n          <div className=\"rounded-lg border p-4\">\n            <h3 className=\"mb-2 text-sm font-medium\">Reports</h3>\n            <p className=\"text-muted-foreground text-sm\">\n              Generate and view custom reports.\n            </p>\n          </div>\n        </TabsContent>\n      </Tabs>\n    </Example>\n  )\n}\n",
      "type": "registry:example"
    }
  ],
  "type": "registry:example"
}
      "path": "registry/radix-maia/examples/alert-dialog-example.tsx",
      "content": "import {\n  AlertDialog,\n  AlertDialogAction,\n  AlertDialogCancel,\n  AlertDialogContent,\n  AlertDialogDescription,\n  AlertDialogFooter,\n  AlertDialogHeader,\n  AlertDialogMedia,\n  AlertDialogTitle,\n  AlertDialogTrigger,\n} from \"@/registry/bases/radix/ui/alert-dialog\"\nimport { Button } from \"@/registry/bases/radix/ui/button\"\nimport { IconPlaceholder } from \"@/app/(design)/design/components/icon-placeholder\"\n\nexport default function AlertDialogDemo() {\n  return (\n    <div className=\"bg-background flex min-h-screen min-w-0 items-center justify-center p-4\">\n      <div className=\"flex flex-wrap gap-4\">\n        <AlertDialogExample1 />\n        <AlertDialogExample2 />\n        <AlertDialogExample3 />\n        <AlertDialogExample4 />\n        <AlertDialogExample5 />\n        <AlertDialogExample6 />\n      </div>\n    </div>\n  )\n}\n\nfunction AlertDialogExample1() {\n  return (\n    <AlertDialog>\n      <AlertDialogTrigger asChild>\n        <Button variant=\"outline\">Default</Button>\n      </AlertDialogTrigger>\n      <AlertDialogContent>\n        <AlertDialogHeader>\n          <AlertDialogTitle>Are you absolutely sure?</AlertDialogTitle>\n          <AlertDialogDescription>\n            This action cannot be undone. This will permanently delete your\n            account and remove your data from our servers.\n          </AlertDialogDescription>\n        </AlertDialogHeader>\n        <AlertDialogFooter>\n          <AlertDialogCancel>Cancel</AlertDialogCancel>\n          <AlertDialogAction>Continue</AlertDialogAction>\n        </AlertDialogFooter>\n      </AlertDialogContent>\n    </AlertDialog>\n  )\n}\n\nfunction AlertDialogExample2() {\n  return (\n    <AlertDialog>\n      <AlertDialogTrigger asChild>\n        <Button>Small</Button>\n      </AlertDialogTrigger>\n      <AlertDialogContent size=\"sm\">\n        <AlertDialogHeader>\n          <AlertDialogTitle>Allow accessory to connect?</AlertDialogTitle>\n          <AlertDialogDescription>\n            Do you want to allow the USB accessory to connect to this device?\n          </AlertDialogDescription>\n        </AlertDialogHeader>\n        <AlertDialogFooter>\n          <AlertDialogCancel>Don&apos;t allow</AlertDialogCancel>\n          <AlertDialogAction>Allow</AlertDialogAction>\n        </AlertDialogFooter>\n      </AlertDialogContent>\n    </AlertDialog>\n  )\n}\n\nfunction AlertDialogExample3() {\n  return (\n    <AlertDialog>\n      <AlertDialogTrigger asChild>\n        <Button variant=\"outline\">Default (Media)</Button>\n      </AlertDialogTrigger>\n      <AlertDialogContent>\n        <AlertDialogHeader>\n          <AlertDialogMedia>\n            <IconPlaceholder\n              lucide=\"BluetoothIcon\"\n              tabler=\"IconBluetooth\"\n              hugeicons=\"BluetoothIcon\"\n            />\n          </AlertDialogMedia>\n          <AlertDialogTitle>Are you absolutely sure?</AlertDialogTitle>\n          <AlertDialogDescription>\n            This will permanently delete your account and remove your data from\n            our servers.\n          </AlertDialogDescription>\n        </AlertDialogHeader>\n        <AlertDialogFooter>\n          <AlertDialogCancel>Cancel</AlertDialogCancel>\n          <AlertDialogAction>Continue</AlertDialogAction>\n        </AlertDialogFooter>\n      </AlertDialogContent>\n    </AlertDialog>\n  )\n}\n\nfunction AlertDialogExample4() {\n  return (\n    <AlertDialog>\n      <AlertDialogTrigger asChild>\n        <Button>Small (Media)</Button>\n      </AlertDialogTrigger>\n      <AlertDialogContent size=\"sm\">\n        <AlertDialogHeader>\n          <AlertDialogMedia>\n            <IconPlaceholder\n              lucide=\"BluetoothIcon\"\n              tabler=\"IconBluetooth\"\n              hugeicons=\"BluetoothIcon\"\n            />\n          </AlertDialogMedia>\n          <AlertDialogTitle>Allow accessory to connect?</AlertDialogTitle>\n          <AlertDialogDescription>\n            Do you want to allow the USB accessory to connect to this device?\n          </AlertDialogDescription>\n        </AlertDialogHeader>\n        <AlertDialogFooter>\n          <AlertDialogCancel>Don&apos;t allow</AlertDialogCancel>\n          <AlertDialogAction>Allow</AlertDialogAction>\n        </AlertDialogFooter>\n      </AlertDialogContent>\n    </AlertDialog>\n  )\n}\n\nfunction AlertDialogExample5() {\n  return (\n    <AlertDialog>\n      <AlertDialogTrigger asChild>\n        <Button variant=\"destructive\">Delete Chat</Button>\n      </AlertDialogTrigger>\n      <AlertDialogContent size=\"sm\">\n        <AlertDialogHeader>\n          <AlertDialogMedia className=\"bg-red-50 text-red-500 dark:bg-red-950 dark:text-red-400\">\n            <IconPlaceholder\n              lucide=\"Trash2Icon\"\n              tabler=\"IconTrash\"\n              hugeicons=\"Delete02Icon\"\n            />\n          </AlertDialogMedia>\n          <AlertDialogTitle>Delete chat?</AlertDialogTitle>\n          <AlertDialogDescription>\n            This will permanently delete this chat conversation. View{\" \"}\n            <a href=\"#\">Settings</a> delete any memories saved during this chat.\n          </AlertDialogDescription>\n        </AlertDialogHeader>\n        <AlertDialogFooter>\n          <AlertDialogCancel>Cancel</AlertDialogCancel>\n          <AlertDialogAction asChild>\n            <Button variant=\"destructive\">Delete</Button>\n          </AlertDialogAction>\n        </AlertDialogFooter>\n      </AlertDialogContent>\n    </AlertDialog>\n  )\n}\n\nfunction AlertDialogExample6() {\n  return (\n    <AlertDialog>\n      <AlertDialogTrigger asChild>\n        <Button variant=\"secondary\">\n          <IconPlaceholder\n            lucide=\"HeadphonesIcon\"\n            tabler=\"IconHeadphones\"\n            hugeicons=\"HeadphonesIcon\"\n          />\n          Connect Device\n        </Button>\n      </AlertDialogTrigger>\n      <AlertDialogContent size=\"sm\">\n        <AlertDialogHeader>\n          <AlertDialogTitle>\n            Are you connecting a pair of headphones?\n          </AlertDialogTitle>\n          <AlertDialogDescription>\n            If you switch devices, you can update this selection in the{\" \"}\n            <a href=\"#\">Sounds & Haptics</a> section of Settings.\n          </AlertDialogDescription>\n        </AlertDialogHeader>\n        <AlertDialogFooter>\n          <AlertDialogCancel>Other Device</AlertDialogCancel>\n          <AlertDialogAction>Headphones</AlertDialogAction>\n        </AlertDialogFooter>\n      </AlertDialogContent>\n    </AlertDialog>\n  )\n}\n",
      "type": "registry:example"
    }
  ]
}